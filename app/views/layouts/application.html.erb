<!-- <!DOCTYPE html> -->
<html>
<head>
  <!-- meta character set -->
  <meta charset="utf-8">
  <!-- Always force latest IE rendering engine or request Chrome Frame -->
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <title>ICRTET</title>
  <%= favicon_link_tag 'favicon-32x32.png' %>
  <!-- Mobile Specific Meta -->
  <meta name="viewport" content="width=device-width, initial-scale=1">


  <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,700' rel='stylesheet' type='text/css'>
  <link href="https://fonts.googleapis.com/css?family=Russo+One" rel="stylesheet">

  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
  <link href="https://fonts.googleapis.com/css?family=Orbitron" rel="stylesheet">
  <%= stylesheet_link_tag 'animate.css' %>
  <%= stylesheet_link_tag 'main1.css' %>
  


  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %> 
  <%= javascript_include_tag 'modernizr-2.6.2.min.js', 'data-turbolinks-track' => true %>
  <script src="http://maps.google.com/maps/api/js?sensor=false"></script>
  <%= javascript_include_tag 'new.js', 'data-turbolinks-track' => true %>


  <%= csrf_meta_tags %>


  <style type="text/css">

  </style>

</head>
<body>

  <% if params[:action] != "pdf" %>
  <div class="col-md-2" style="padding-left: 30px;">
    <%= image_tag "clglogo.png" , style: "height: 120px; width: 110px;" %>
  </div>
  <div class="col-md-2 pull-right">
    <%= image_tag "icrtet17.png", style: "height: 120px; width: 110px;" %>
  </div>


  <div class="col-md-8">
    <h1 class="center"  style="font-family:'Russo One', sans-serif;font-size:30px;color:white;text-align: center;font-variant: regular;"> St. Joseph's Institute of Technology</h1> 
  </div>
  <div class="col-md-8" align="center">
    <%= image_tag "IEEE_logo.png" , style: "height: 60px; width: 110px;" %>
  </div>
  <div class="col-md-12">
    <h2 class="center"  style="font-family:'Russo One', sans-serif;color:white; text-align: center; font-size: 22px;">2 <sup>nd</sup> International Conference on 
      <center class="center"  style="font-family:'Russo One', sans-serif;color:white; text-align: center; font-size: 22px;">Recent Trends in Engineering and Technology</center> </h2>
 <!--      <h6 class="center"  style="font-family:'Orbitron', sans-serif;color:white;text-align: center;"> In Association with</h6> 
-->
</div>

<style type="text/css">
/*  .navbar,.nav,.navbar-inverse,.navbar-static-top,.navbar-header,. {
    background-color: white;
  } */
</style>
        <!--
        Fixed Navigation
        ==================================== -->
        
        <header id="navigation" class="navbar-inverse navbar-static-top">

          <div class="navbar-header" >
            <!-- responsive nav button -->
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
              <span class="sr-only">Toggle navigation</span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
            </button>

            <!-- /responsive nav button -->
          </div>
          <!-- logo -->


          <nav class="collapse navbar-collapse navbar-static-top" role="navigation">

           <ul id="nav" class="nav navbar-nav" style="padding-left: 200px; padding-top: 8px;">
            <li class=<%if controller.action_name == "home" %>"active" <%end%> ><%= link_to "Home",home_path %></li>
            <li class=<%if controller.action_name == "call_for_papers" %>"active" <%end%> ><%= link_to "Info Log", call_for_papers_path %></li>
            <li class=<%if controller.action_name == "important_dates" %>"active" <%end%> ><%= link_to "Important Dates", important_dates_path %></li>
            <li class=<%if controller.action_name == "publications" %>"active" <%end%> ><%= link_to "Publications",publications_path %></li>
            <li class=<%if controller.action_name == "commitee" %>"active" <%end%> ><%= link_to "Committee", commitee_path %></li>
            <li class=<%if controller.action_name == "venue_and_contact" %>"active" <%end%> ><%= link_to "Venue and Contact", venue_and_contact_path %></li>
            <% if !user_signed_in? %>
            <li class=<%if controller.action_name == "new" %>"active" <%end%> ><%= link_to "Sign In / Register", new_user_session_path %></li>

            <% elsif current_user.admin? %>
            <li class=<%if controller.action_name == "dash" %> "active" <%end%> ><%= link_to "Admin", dashboard_dash_path %></li>
            <li><%= link_to "Log out", destroy_user_session_path, :method => :delete %></li>
            <% else %>
            <li class=<%if controller.action_name == "index" %>"active" <%end%> ><%= link_to "Upload your paper", papers_path %></li>

            <li class=<%if controller.action_name == "new" %>"active" <%end%> ><%= link_to "Event Registration", payment_details_path %></li>
            <li class=<%if controller.action_name == "new" %>"active" <%end%> ><%= link_to "Journal Payment", journalpayments_path %></li>
            <li><%= link_to "Log out", destroy_user_session_path, :method => :delete %></li>
            <% end %>
          </ul>
        </nav>
        <!-- /main nav -->


      </header>
      

      <!-- <div id="fix-for-navbar-fixed-top-spacing" style="height: 210px;">&nbsp;</div> -->
      <!-- <p class="microsoft marquee">Icrtet 2017</p> -->
      

      <div class="ticker-container">
        <div class="ticker-caption">
          <p>INFO</p>
        </div>
        <ul>
          <div class="ticker-active">
            <li><span>Last date for submitting papers is <strike style="color:red;">5 April 2017</strike>  <strong>18 April 2017</strong></span></li>
          </div>
          <div class="not-active">
            <li><span>All accepted papers will be publised in Annexure-I (SCI indexed)/Annexure-II(SCOPUS indexed)</span></li>
          </div>
          <div class="not-active">
            <li><span>For any queries mail to icrtet2017@stjosephstechnology.ac.in <span style="color : red;">(Don't send any papers to this e-mail).</span></span></li>
          </div>
          <div class="not-active">
            <li><span>Register and Upload your papers by clicking this link. <%= link_to "Sign In / Register", new_user_registration_path %></span></li>
          </div>
<!--           <div>
            <li><span>Breaking News 5 &ndash; <a href="#">read more</a></span></li>
          </div> -->
        </ul>
      </div>

      <%= render 'layouts/messages' %>
      <!-- <marquee class="li" direction=”right” ><h4 style="color=blue;">★ Last date for submitting papers is 5 April 2017 ★ All accepted papers will be publised in Annexure-I (SCI indexed)/Annexure-II (SCOPUS indexed) ★</h4></marquee> -->
      <% end %>

      <%= yield %>
      <%= any_login_here if defined?(AnyLogin) %>
      <% if params[:action] != "pdf" %>



      <footer id="footer" >
        <div class="container">
          <div class="row">
            <div class="col-sm-6">
              &copy; 2017 <a target="_blank" href="http://stjosephstechnology.ac.in/" title="St. Joseph's Institute of Technology">St. Joseph's Institute of Technology</a>. All Rights Reserved.
            </div>
          </div>
        </div>

      </footer><%end%>
    </body>
    </html>