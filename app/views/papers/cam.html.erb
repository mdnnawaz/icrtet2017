<div class = "container">
	<h1>Add Camera Ready Paper</h1>
	<div class="col-md-4"></div>
	<div class="col-md-4 text-center">
		<%= form_for(@paper) do |f| %>
			<div class="field">
		    	<%= f.label :camera_ready_paper %><br>
		    	<%= f.file_field :camera_ready_paper, class:"form-control" %>
			</div>
			<div class="actions">
		    	<%= f.submit "Add Paper",class: "btn btn-success" %>
			</div>
		<% end %>
		<%= link_to 'Back', papers_path %>
	</div>
</div>
